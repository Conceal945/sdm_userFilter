{
    "version": 5,
    "identifier": "mobileqq.imageCache.scuf.sdm",
    "color": "#F05E1C",
    "rootOnly": false,
    "label": "腾讯QQ 图像缓存",
    "description": "对话时加载到本地的图片缓存\n注意：并非用户手动保存到相册的图片",
    "mainPath": [
        "\/storage\/emulated\/0\/Android\/data\/com.tencent.mobileqq",
        "\/storage\/emulated\/0\/Tencent\/",
        "\/storage\/emulated\/0\/tencent\/"
    ],
    "pathContains": [
        "MobileQQ\/thumb",
        "MobileQQ\/diskcache",
        "MobileQQ\/photo",
        "MobileQQ\/chatpic",
        "MobileQQ\/qbiz",
        "QQfile_recv\/.thumbnails",
        "QQfile_recv\/.tmp",
        "QQfile_recv\/.trooptmp"
    ],
    "exclusions": [
        ".nomedia"
    ]
}